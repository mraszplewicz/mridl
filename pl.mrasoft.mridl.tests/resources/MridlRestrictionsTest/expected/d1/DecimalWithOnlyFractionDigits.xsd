<?xml version="1.0" encoding="utf-8"?>
<xs:schema elementFormDefault="unqualified" 
		   xmlns:xs="http://www.w3.org/2001/XMLSchema"
		   xmlns:tns="http://mrasoft.pl/mridl/examples/d1/DecimalWithOnlyFractionDigits"
		   targetNamespace="http://mrasoft.pl/mridl/examples/d1/DecimalWithOnlyFractionDigits">
